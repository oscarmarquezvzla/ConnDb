<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gedica.conndb.AlmEntOtrosIngresos">

    <TextView
        android:id="@+id/tv1"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="22dp"
        android:text="Centro"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/et1"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:ems="10"
        android:inputType="number"
        android:layout_alignTop="@+id/tv1"
        android:layout_toEndOf="@+id/tv1" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_alignTop="@+id/et1"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/et1"
        android:text="Almacen" />

    <EditText
        android:id="@+id/et2"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:ems="10"
        android:inputType="number"
        android:layout_alignTop="@+id/tv2"
        android:layout_toEndOf="@+id/tv2"
        android:layout_marginStart="18dp" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_alignStart="@+id/tv1"
        android:layout_below="@+id/tv1"
        android:layout_marginTop="38dp"
        android:text="Material" />

    <EditText
        android:id="@+id/et3"
        android:layout_width="180dp"
        android:layout_height="20dp"
        android:ems="10"
        android:inputType="number"
        android:layout_alignTop="@+id/tv3"
        android:layout_toStartOf="@+id/et2" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="80sp"
        android:layout_height="20sp"
        android:layout_alignStart="@+id/tv3"
        android:layout_below="@+id/tv3"
        android:layout_marginTop="40dp"
        android:text="Desc. Mat" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:layout_alignTop="@+id/tv4"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_alignStart="@+id/tv4"
        android:layout_below="@+id/tv4"
        android:layout_marginTop="39dp"
        android:text="Lote" />

    <Spinner
        android:id="@+id/sp1"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:layout_alignTop="@+id/tv5"
        android:layout_toEndOf="@+id/tv4" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_alignStart="@+id/tv5"
        android:layout_below="@+id/tv5"
        android:layout_marginTop="42dp"
        android:text="Cantidad" />

    <EditText
        android:id="@+id/et5"
        android:layout_width="140dp"
        android:layout_height="20dp"
        android:layout_alignStart="@+id/sp1"
        android:layout_alignTop="@+id/tv6"
        android:ems="10"
        android:inputType="number" />

    <TextView
        android:id="@+id/tv7"
        android:layout_width="100sp"
        android:layout_height="20sp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tv6"
        android:layout_marginBottom="29dp"
        android:text="Registro Ant." />

    <EditText
        android:id="@+id/et6"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:ems="10"
        android:inputType="number"
        android:layout_alignTop="@+id/tv7"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
